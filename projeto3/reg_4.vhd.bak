library ieee;
use ieee.std_logic_1164.all;

entity reg_4 is
port(	i_I4:		in std_logic_vector (7 downto 0);	-- entrada regsitrador
		i_EN4:	in std_logic;								-- enable
		o_Q4:		out std_logic_vector (7 downto 0));	-- sa√≠da reg
end reg_4;

architecture arch_5 of reg_4 is
begin
	process(i_I4, i_EN4)
		begin
		  if(i_EN4 = '1') then
			 o_Q4 <= i_I4;
		end if;
	end process;
end arch_5;